<?xml version="1.0" encoding="utf-8"?>
<odoo>
      <record id="inglot_pos_config_view_form" model="ir.ui.view">
          <field name="name">inglot.pos.config.form.viewt</field>
          <field name="model">pos.config</field>
          <field name="inherit_id" ref="point_of_sale.pos_config_view_form"/>
          <field name="arch" type="xml">
              <sheet position="inside">
                  <div class="row mt16 o_settings_container" id="Plantllas">
                      <div class="col-12 col-lg-6 o_setting_box">
                          <div class="o_setting_right_pane">
                            <label for="plantilla_correo_id" string="Plantilla"/>
                            <field name="plantilla_correo_id"/>
                          </div>
                      </div>
                  </div>
              </sheet>
          </field>
      </record>
</odoo>
